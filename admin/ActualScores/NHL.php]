<?php
	
	$html = file_get_contents("https://www.hockey-reference.com/boxscores/index.fcgi?month=10&day=23&year=2019");
	$start = strpos($html, "Find Games");
	$end =   strpos($html, "dailyleaders");
	$only =  substr($html, $start, $end-$start);
//	echo $only;
	echo "<br>";

	$pattern = '#<td><a href="/teams/(.*?)2020.html#';
	preg_match_all($pattern, $only, $matches);
	print_r($matches);

	$pattern = '<td class="right">(.*?)</td>#';
	preg_match_all($pattern, $only, $matches);
        print_r($matches);


?>
